{
  "name": "server",
  "version": "1.0.0",
  "description": "Part.ai Fullstack Program",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "server": "nodemon ./server.js --exec babel-node --experimental-modules -e js --ignore client",
    "client": "npm start --prefix client",
    "graphql": "nodemon ./server.js --exec babel-node --experimental-modules -e js",
    "start": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "graphql": "^15.8.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.10.0",
    "nodemon": "^2.0.20",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.0",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "concurrently": "^7.6.0"
  }
}
